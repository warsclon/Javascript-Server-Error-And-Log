<!DOCTYPE html>
 <html> 
  <head>
    <title>Detail Javascript Error Server</title>
    <!-- Bootstrap -->
    <link href="/css/bootstrap.css" rel="stylesheet" media="screen">
    <link href="/css/bootstrap.min.css" rel="stylesheet" media="screen">
	<link href="/css/bootstrap-responsive.css" rel="stylesheet">
<script>
function myFunction() {

	$('#myModal').modal('show');

}

function close() {

	$('#myModal').modal('hide');
}

function deleteYes() {
	close();
	window.location = "/logs/<%= appid %>/<%= log._id %>/delete";
}
</script>
  </head>

<body> 


<div class="navbar">
  <div class="navbar-inner">
    <a class="brand" href="#">Detail</a>
    <ul class="nav">
	  <li><a href="/apps">Select App</a></li>
	  <li><a href="/logs/<%= appid %>">Logs App</a></li>
	  <li><a href="/logsexport/<%= appid %>/<%= log._id  %>">Export log (json format)</a></li>
	  <li ><a href="/logout">Logout</a></li>
  </div>
</div>

<p>
<div class="container">
<div style="margin-top:10px;margin-bottom:10px;margin-right:20px;margin-left:20px;">
<br>
    <button class="btn btn-primary" type="button" onclick="javascript:myFunction()">
      Delete
    </button>
<p>

<h3>Info Event:</h3>
<p>
    <table class="table table-striped">
        <tbody>
    <tr>
        <td>
            <b>url</b>
        </td>
        <td>
            <%= log.url %>
        </td>
    </tr>
    <tr>
        <td>
            <b>lineNumber</b>
        </td>
        <td>
            <%= log.lineNumber %>
        </td>
    </tr>
    <tr>
        <td>
            <b>message</b>
        </td>
        <td>
            <%= log.message %>
        </td>
    </tr>
    </tbody>
</table>

<h3>Info browser:</h3>
<p>

<table class="table table-striped">
<tbody>
<tr>
    <td>
        <b>app</b>
    </td>
    <td>
        <%= log.app %>
    </td>
</tr>
<tr>
    <td>
